{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome against localhost, with sourcemaps",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "Attach to Chrome, with sourcemaps",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "Amelia",
      "type": "chrome",
      "request": "launch",
      "port": 9222,
      "url": "http://localhost/amelia/sc/resp/index.htm",
      "sourceMaps": true,
      "webRoot": "C:\\Projects\\Garage\\amelia\\sc\\"
    }
  ]
}